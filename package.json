  "description": "",
  "devDependencies": {
    "@parcel/packager-raw-url": "^2.16.0",
    "@parcel/transformer-sass": "^2.16.0",
    "@parcel/transformer-webmanifest": "^2.16.0",
    "npm-run-all": "^4.1.5",
    "parcel": "latest",
    "rimraf": "^6.1.0",
    "sass": "^1.93.3",
    "sharp": "^0.33.5"
  },
  "dependencies": {
    "aos": "^2.3.4",
    "typed.js": "^2.1.0"
  }
