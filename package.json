{
  "name": "pec1",
  "version": "1.0.0",
  "source": "src/html/**/*.html",
  "browserslist": "> 0.5%, last 2 versions, not dead, not op_mini all",
  "scripts": {
    "start": "npm-run-all clean parcel:dev",
    "build": "npm-run-all clean parcel:build copy:redirects",
    "parcel:dev": "parcel",
    "parcel:build": "parcel build src/html/index.html src/html/categoria/*.html src/html/det/*.html src/html/enlaces/*.html",
    "clean": "rimraf dist .parcel-cache",
    "copy:redirects": "cp _redirects dist/ || copy _redirects dist\\"
  },
